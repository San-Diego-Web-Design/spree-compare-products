<table class="index">
  <tr>
    <th><%= t("name") %></th>
    <th><%= t("comparable_container") %></th>
    <th></th>
  </tr>
<% for taxonomy in @taxonomies %>
  <tr id="<%= dom_id taxonomy %>">
    <td><%= taxonomy.name %></td>
    <td><%= icon('tick') if taxonomy.comparable? %></td>
    <td class="actions">
      <%= link_to_edit taxonomy.id %> &nbsp; <%= link_to_delete taxonomy %>
    </td>
  </tr>
<% end %>
</table>
